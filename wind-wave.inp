&BODY

    Material_number  = 1
    Surface_Name     = 'box'
    Fill             = 'inside'
    Length           = 1e5, 1e5, 10.0
    Translation_Pt   = 0.0, 0.0, 5.0
    Velocity         = 0.0, 0.0, 0.0
    Temperature      = 273.0

/


&BODY

    Material_number  = 2
    Surface_Name     = 'background'
    Velocity         = 0.0, 0.0, 0.0
    Temperature      = 273.0

/

&MATERIAL

    Material_Name           = 'water'
    Material_Number         = 1
    priority                = 1

    Density                 = 1000.0

    Cp_Relation             = 'constant'
    Cp_Constants            = 1.0
    Cp_Exponents            = 0.0
    Conductivity_Relation   = 'constant'
    Conductivity_Constants  = 1.0
    Conductivity_Exponents  = 0.0

    Viscosity_Relation      = 'constant'
    Viscosity_Constants     = 1e-3
    Viscosity_Exponents     = 0.0

/

&MATERIAL

    Material_Name           = 'air'
    Material_Number         = 2
    Material_Feature        = 'background'
    priority                = 2

    Density                 = 1.2

    Cp_Relation             = 'constant'
    Cp_Constants            = 1.0
    Cp_Exponents            = 0.0

    Viscosity_Relation      = 'constant'
    Viscosity_Constants     = 1e-3
    Viscosity_Exponents     = 0.0

    Conductivity_Relation   = 'constant'
    Conductivity_Constants  = 1.0
    Conductivity_Exponents  = 0.0

/

&PARALLEL_PARAMETERS

    Partitioner  = 'automatic',

/
